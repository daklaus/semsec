\ProvidesPackage{beamercolorthemeINSO}[2012/12/02 v. 0.01]

\mode<presentation>

\definecolor{INSOred}{rgb}{.78,0,.19}
\definecolor{INSOgray}{rgb}{.37,.37,.37}
\definecolor{INSOblack}{rgb}{.0,.0,.0}
\definecolor{INSOwhite}{rgb}{1,1,1}

%

\setbeamercolor*{frametitle}{fg=INSOwhite,bg=INSOred}
\setbeamercolor*{headline}{use=frametitle}
\setbeamercolor*{footline}{fg=INSOgray}
% 
\setbeamercolor*{normal text}{fg=INSOgray}
\setbeamercolor*{alerted text}{fg=INSOred}
\setbeamercolor*{item}{fg=INSOred}
\setbeamercolor*{subitem}{fg=INSOgray}
\setbeamercolor*{subsubitem}{use=itemize subitem}
% 
\setbeamercolor*{titlelike}{fg=INSOred}
\setbeamercolor*{structure}{fg=INSOgray}

\mode<all>

